generator custom_generator {
  provider = "node ../../node_modules/prisma-generator-seeder"
  output   = "../types"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model TestParent {
  id       String      @id @default(uuid())
  title    String
  elements TestChild[]
}

model TestChild {
  id          String     @id @default(uuid())
  title       String
  containerId String
  TestParent  TestParent @relation(fields: [containerId], references: [id])
}
