generator custom_generator {
  provider = "node ../../node_modules/prisma-generator-seeder"
  output   = "../types"
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["metrics", "views"]
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model TestParent {
  id       String      @id @default(uuid())
  title    String
  elements TestChild[]
}

model TestChild {
  id         String     @id @default(uuid())
  title      String
  parentId   String
  TestParent TestParent @relation(fields: [parentId], references: [id])
}
